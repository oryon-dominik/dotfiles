[user]
        email = oryon@cyberise.de
        name = oryon-dominik
[core]
        sshCommand = ssh -i ~/.ssh/id_rsa_GITHUB
        autocrlf = input
        editor = code --wait
[merge]
    tool = vscode
[mergetool "vscode"]
    cmd = code --wait $MERGED
[diff]
    tool = vscode
[difftool "vscode"]
        cmd = code --wait --diff $LOCAL $REMOTE
